<div class="container">
<h1>Incidents</h1>

<%= form_for @incident, class: "form-horizontal" do |f| %>
</br>


<%= f.label :number %>
<%= f.text_field :number, class: 'form-control' %>
</br>

<%= f.label :caller %>
<%= f.text_field :caller, class: 'form-control' %>

</br>
<%= f.label :location %> 
<%= f.text_field :location, class: 'form-control' %>
</br>

<%= f.label :category %> 
<%= f.text_field :category, class: 'form-control' %>
</br>

<%= f.label :email %>
<%= f.text_field :email, class: 'form-control' %>
</br>

<%= f.label :shortdescription %>
<%= f.text_field :shortdescription, class: 'form-control' %>
</br>

<%= f.label :state %>
<%= f.text_field :state, class: 'form-control' %>
</br>

<%= f.label :contact_type %>
<%= f.text_field :contact_type, class: 'form-control' %>
</br>

<%= f.submit "Add incidents!" %>
<% end %>
<%= link_to "Back to the list!", incidents_path %>
</div>